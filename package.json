{
  "name": "i-finance",
  "private": true,
  "eslint": "./node_modules/.bin/eslint",
  "dependencies": {
    "@apollo/client": "3.7.11",
    "@hotwired/turbo-rails": "7.3.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.14",
    "esbuild": "0.16.17",
    "graphql": "16.6.0",
    "graphql-ruby-client": "1.11.7",
    "graphql-tag": "2.12.6",
    "jwt-decode": "3.1.2",
    "postcss": "8.4.21",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "15.1.0",
    "postcss-nested": "6.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.10.0",
    "react-router-dom": "6.10.0",
    "react-router-v6-instrumentation": "2.0.2",
    "tailwindcss": "3.3.1"
  },
  "scripts": {
    "build": "NODE_PATH=app/javascript/components esbuild app/javascript/*.* --bundle --outdir=app/assets/builds --sourcemap --loader:.svg=file --loader:.png=file --loader:.jpg=file --public-path=$CDN_URL_FULL/assets --asset-names=[name]-[hash].digested --target=chrome72,firefox74,safari13,edge19 --minify",
    "build:dev": "NODE_PATH=app/javascript/components esbuild app/javascript/*.* --bundle --outdir=app/assets/builds --sourcemap --loader:.svg=file --loader:.png=file --loader:.jpg=file --public-path=/assets --asset-names=[name]-[hash].digested --target=chrome72,firefox74,safari13,edge19",
    "build:css": "tailwindcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
    "lint": "yarn run eslint --ext .js --ext .jsx --ext .gql --ext .graphql app/javascript",
    "graphql:dump_schema": "rails graphql:dump_schema"
  },
  "devDependencies": {
    "@prettier/plugin-ruby": "3.2.2",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.11.0",
    "eslint-plugin-testing-library": "5.10.2",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.7"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true
  }
}
